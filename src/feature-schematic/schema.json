{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "feature-schematic",
    "title": "Feature generator",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1,
            "default": "",
            "x-prompt": "Input the feature name"
        },
        "destination": {
            "type": "string",
            "default": "feautres",
            "enum": [
              "features",
              "pages"
            ],
            "x-prompt": "Select the destination"
          }
    }
}
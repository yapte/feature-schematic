{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "feature-schematic": {
      "description": "A blank schematic.",
      "factory": "./feature-schematic/index#featureSchematic",
      "schema": "./feature-schematic/schema.json"
    },
    "cui": {
      "description": "Conundrum feature schematic",
      "factory": "./cui/index#cuiSchematic",
      "schema": "./cui/schema.json"
    },
    "cui-list": {
      "description": "Conundrum feature with list schematic",
      "factory": "./cui-list/index#cuiListSchematic",
      "schema": "./cui-list/schema.json"
    },
    "linde-mini": {
      "description": "Linde feature schematic",
      "factory": "./linde-mini/index#lindeMini",
      "schema": "./linde-mini/schema.json"
    }
  }
}
